<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Add Button in Tree View Action Menu -->
    <record id="kanhasoft_account_move_tree_inherit" model="ir.ui.view">
        <field name="name">kanhasoft.account.move.tree.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_out_invoice_tree"/>
        <field name="arch" type="xml">

            <xpath expr="//tree" position="inside">
                <button name="action_bulk_force_delete"
                        type="object"
                        string="Force Delete"
                        class="btn-danger"/>
            </xpath>

        </field>
    </record>

</odoo>